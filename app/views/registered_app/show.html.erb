<h1><%= @registeredapp.name %></h1>
  <div class="row">
    <div class="col-md-8">
      <p>URL: <%= link_to @registeredapp.url, "http://#{@registeredapp.url}" %></p>
      <p>Added by <%= @registeredapp.user.email %></p>
    </div>
    <div class="col-md-4">
      <%= link_to "Edit", edit_registered_app_path, class: 'btn btn-success' %>
      <%= link_to "Delete", [@registeredapp], method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you want to delete this app?'} %>
    </div>
  </div>
